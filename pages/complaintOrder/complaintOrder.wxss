/**index.wxss**/
@import "/utils/common.wxss";
.container{
  padding-bottom:16rpx;
  font-size: 34rpx;
  border-top: 1rpx solid #eeeeee;
}
.text_list{
  display: flex;
  display: -webkit-flex;
  width: 90%;
  font-size:30rpx;
  padding: 20px 5%;
  border-bottom: 1rpx solid #eeeeee;
}
.text_list .text_tit{
  width: 5%;
}
.text_list .text_con{
  width: 90%;
}
.text_list .text_check{
  width: 5%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.mess_info{
  padding: 6px 5%;
  border-bottom: 1rpx solid #eeeeee;
}
.mess_info .order_id{
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  padding: 6px 0;
}
.order_sender{
  
}
.title{
  padding: 10px 0;
  padding-left:5%;
  font-size: 16px;
  border-bottom: 1rpx solid #eeeeee;
}

.form_btn_box{
  display: flex;
  display: -webkit-flex;
  margin:20rpx auto;
}
.form_btn_box .form_btn{
  margin-top: 32rpx;
  width:260rpx;
  height:80rpx;
  line-height:80rpx;
  font-size: 36rpx;
  color: #ffffff;
  border: none;
  border-radius: 50rpx;
  border-color: #758da6;
  background: #758da6;
}