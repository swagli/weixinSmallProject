/**wxss**/
@import "/utils/common.wxss";
page{
  background-color: #f4f4f4;
}
.container{
  padding-top: 20rpx;
  font-size: 34rpx;
  height: 100%;
  width:100%;
  background: #f4f4f4;
}
.red_box{
  padding: 0 15rpx;
}
.red_box_list{
  display: flex;
  display: -webkit-flex;
  justify-content: flex-start;
  margin-bottom:14rpx;
  height: 170rpx;
  font-size: 24rpx;
  color: #999999;
  background: url(data:image/png;base64,) no-repeat left;
  background-size: 100%;
}
.red_box_list .red_left .money_tit{
  padding: 20rpx 0;
  color: #e4393c;
}
.red_box_list .red_left .money_tit .moneyNumber{
  font-size: 40rpx;
  font-weight: 600;
  margin-left:6rpx;
}
.red_box_list .red_left{
  width: 26%;
  text-align: center;
  padding:10rpx 0 0 20rpx;
}
.red_box_list .red_center{
  width: 40%;
  padding:10rpx 0 0 60rpx;
}
.red_box_list .red_center .money_tit{
  padding:20rpx 0;
  padding-bottom:0rpx;
  font-size: 30rpx;
  color: #000000;
}
.red_box_list .red_center .money_tit .couponName{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.red_box_list .red_center .money_type{
  margin: 6rpx 0;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.red_box_list .red_right{
  width: 20%;
  text-align: center;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
}
.red_box_list .red_right .used{
  padding: 4rpx 20rpx;
  background: #e4393c;
  border-radius: 50rpx;
  color: #ffffff;
}
.notUse{
  font-size: 28rpx;
  padding: 18rpx 60rpx;
  padding-right: 80rpx;
  margin: 0;
  margin-bottom: 16rpx;
  background: #ffffff;
  display: flex;
  justify-content: space-between;
  align-items: center;
}