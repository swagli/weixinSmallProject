/* pages/balanceRecord/balanceRecord.wxss */
@import "/utils/common.wxss";
.image_gif{
  width: 100%;
}
.number_box{
  display: flex;
  display: -webkit-flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
}
.number_box .number_list{
  width:26%;
  text-align: center;
  padding: 8px 0;
  margin: 16px 3%;
  font-size: 18px;
  margin-bottom: 0;
  border-radius: 6px;
  border: 1px solid #eeeeee;
}
.number_box .list_active{
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100% 100%;
}
.title{
  margin-top: 10px;
  padding: 10px 14px;
  font-size: 16px;
}
.preview_money{
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
  padding: 10px 14px;
}
.preview_money .inp_money{
  display: flex;
  display: -webkit-flex;
  flex-wrap: nowrap;
  align-items: center;
}
.preview_money .inp_money .inp_number{
  width: 100px;
  padding: 4px 0px;
  padding-left:12px; 
  margin-right: 10px;
  border-radius: 4px;
  border: 1px solid #eeeeee;
}
.preview_money .money_number{
  color: #ff6600;
  padding: 0 4px;
  margin: 0 2px;
  font-size: 18px;
}
.pay_type{
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: space-between;
  padding: 6px 14px;
  margin-top: 20px;
  border-top: 1px solid #eeeeee;
  border-bottom: 1px solid #eeeeee;
}
.pay_type .pay_title{
  display: flex;
  display: -webkit-flex;
  align-items: center;
}
.pay_type .pay_title text{
  margin-left: 10px;
}
.pay_type .image_tit_weixin{
  width: 30px;
}
.sure_submit{
  width: 120px;
  line-height: 2;
  color: #ffffff;
  margin-top: 50px;
  background: #73bff9;
}