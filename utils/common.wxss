page{
  background: #ffffff;
}
.container{
  font-size: 16px;
  color: #000;
  background: #ffffff;
}
button::after{ border: none; }
.null_data{color: #999999;font-size: 16px;padding-left: 6rpx;}
.no_data .no_data_icon{
  width: 100%;
  height: 164px;
  margin: 60px auto;
  margin-bottom: 60px;
}
.no_data .no_data_text{
  text-align: center;
  color:#666666;
  font-size:16px;
}
.image_show{
  width: 100%;
  height:80px;
  margin-top: 4px;
}
/*底部按钮*/
.form_btn_box{
  display: flex;
  display: -webkit-flex;
  margin:20rpx auto;
}
.form_btn_box .form_btn{
  margin-top: 32rpx;
  width:260rpx;
  height:80rpx;
  line-height:80rpx;
  font-size: 36rpx;
  color: #ffffff;
  border: none;
  border-radius: 50rpx;
  border-color: #758da6;
  background: #1e82d2;
}
/*文字说明*/
.words{
  padding: 10px 12px;
  font-size: 14px;
  color: #666666;
  border-top: 1px solid #eeeeee;
}
.words .words-title{
  font-size: 16px;
  color: #000000;
  margin: 10px 0;
}
.words .words-cont{
  line-height: 1.5;
}