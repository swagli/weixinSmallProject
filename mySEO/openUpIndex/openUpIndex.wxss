/* mySEO/openUpIndex/openUpIndex.wxss */
@import "/utils/common.wxss";
.container{
  font-size: 18px;
}
.banner_box{
  width: 100%;
}
.swiper_box{
  width: 100%;
  height: 200px;
}
.banner_box image, .banner_box .slide-image{
  width: 100%;
}
.register{
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
  margin: 10px auto;
  width: 100%;
}
.register input{
  padding: 0;
  margin: 0;
  padding-left: 12px;
  margin-left:10px;
  border-bottom: 1px solid #eeeeee;
}
.register .name_inp{
  width: 200px;
}
.form_btn_box{
  /* display: flex; */
  display: -webkit-flex;
  margin:20rpx auto;
}
.form_btn_box .form_btn{
  margin-top: 32rpx;
  width:260rpx;
  height:80rpx;
  line-height:80rpx;
  font-size: 36rpx;
  color: #ffffff;
  border: none;
  border-radius: 50rpx;
  border-color: #758da6;
  background: #758da6;
}
.words{
  padding: 10px 12px;
  font-size: 14px;
  color: #666666;
}
.words .words-title{
  font-size: 16px;
  color: #000000;
  margin: 10px 0;
}